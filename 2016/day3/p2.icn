procedure main(args)
	inf := open(args[1],"r")
	valid := 0
	ws := '\t ,'
	indata := list()
	cols := [[],[],[]]
	while line := !inf do {
		line ? {

			tab(many(ws))
			in := []
			while put(in,integer(tab(upto(' \t,')|0))) do
				tab(many(ws))

			every i := 1 to 3 do
				put(cols[i],in[i])

			if *cols[1] = 3 then {
				put(indata,copy(cols))
				cols := [[],[],[]]
			}
		}
	}
	every cols := !indata do {
		every t := !cols do {
			t := sort(t)
			if t[1] + t[2] > t[3] then valid +:= 1
		}
	}

	write(valid)
end